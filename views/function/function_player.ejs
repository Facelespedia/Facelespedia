<script>
    $(document).ready(function(){

     var player_table_global = "";
     var player_table_sea = "";
     var player_table_europe = "";
     var player_table_america = "";
     var player_table_china = "";

        for(var i = 0 ;i<10; i++){
            player_table_global +="<tr><td><a>"+getPlayerSortMMR.PlayerSortMMR[i].TeamName+"</a></td></tr>";
