            player_table_china +="<tr><td><a>"+getTeamChina.TeamChina[i].TeamName+"</a></td></tr>";
        }


    $('#player-global').click(function() { 
        document.getElementById('player-table').innerHTML = player_table_global;
        $(".team-menu").hide();
    });
    $('#player-sea').click(function() { 
        document.getElementById('player-table').innerHTML = player_table_sea;
        $(".player-menu").hide();
    });
    $('#team-china').click(function() { 
        document.getElementById('player-table').innerHTML = player_table_china;
        $(".player-menu").hide();
    });
    $('#player-america').click(function() { 
        document.getElementById('player-table').innerHTML = player_table_america;
        $(".player-menu").hide();
    });
    $('#player-europe').click(function() { 
        document.getElementById('player-table').innerHTML = player_table_europe;
        $(".player-menu").hide();
    });
    $('#player-table').click(function() {
        $('.player-menu').toggle("slide");
        // $('.player-menu').load("/team_card");
    });
    })
</script>