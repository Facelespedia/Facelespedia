<script>

    $("#search-submit").click(function() {
        
        var teamID=-1;
        for(var i=0; i<getTeam.Team.length; i++) {
            if(getTeam.Team[i].TeamsubName == $("#search-input").val() || getTeam.Team[i].TeamName == $("#search-input").val()) {
                teamID = getTeam.Team[i].TeamID;
                break;
            }
        }
        if(teamID != -1) {
            window.location.href = ("#team-rank");
            SelectTableTeam(teamID);
        }else {
            window.location.href = ("#myPage");
        }

    });


</script>