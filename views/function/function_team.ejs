<script>
    $(document).ready(function(){

     var team_table_global = "";
     var team_table_sea = "";
     var team_table_europe = "";
     var team_table_america = "";
     var team_table_china = "";

        for(var i = 0 ;i<10; i++){
            team_table_global +="<tr><td><a onClick=\"SelectTableTeam("+getTeamSortRating.TeamSortRating[i].TeamID+")\">"+getTeamSortRating.TeamSortRating[i].TeamName+"</a></td></tr>";
            team_table_sea +="<tr><td><a onClick=\"SelectTableTeam("+getTeamSea.TeamSea[i].TeamID+")\">"+getTeamSea.TeamSea[i].TeamName+"</a></td></tr>";
            team_table_europe +="<tr><td><a onClick=\"SelectTableTeam("+getTeamEurope.TeamEurope[i].TeamID+")\">"+getTeamEurope.TeamEurope[i].TeamName+"</a></td></tr>";
            team_table_america +="<tr><td><a onClick=\"SelectTableTeam("+getTeamAmerica.TeamAmerica[i].TeamID+")\">"+getTeamAmerica.TeamAmerica[i].TeamName+"</a></td></tr>";
            team_table_china +="<tr><td><a onClick=\"SelectTableTeam("+getTeamChina.TeamChina[i].TeamID+")\">"+getTeamChina.TeamChina[i].TeamName+"</a></td></tr>";
        }
    $('#team-global').click(function() {
        document.getElementById('team-table').innerHTML = team_table_global;
        $(".team-menu").hide();
    });
    $('#team-sea').click(function() { 
        document.getElementById('team-table').innerHTML = team_table_sea;
        $(".team-menu").hide();
    });
    $('#team-china').click(function() { 
        document.getElementById('team-table').innerHTML = team_table_china;
        $(".team-menu").hide();
    });
    $('#team-america').click(function() { 
        document.getElementById('team-table').innerHTML = team_table_america;
        $(".team-menu").hide();
    });
    $('#team-europe').click(function() { 
        document.getElementById('team-table').innerHTML = team_table_europe;
        $(".team-menu").hide();
    });

})
var tempTeam = 0;
function SelectTableTeam(TeamID){
    if(TeamID!=tempTeam){
        createCardTeam(TeamID);
        $('.team-menu').show();
        tempTeam = TeamID;
    }else{
        $('.team-menu').toggle("slide");
    }
}


</script>